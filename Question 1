

#include<iostream>
#include<conio.h>
using namespace std;
class Complex
{
    private:
       int real;
       int img;
    public:
       void set_data(int x,int y)
       {
          real = x;
          img = y;
       }
       void print_data()
       {
          cout<<real<<" + "<<img<<"i";
          cout<<endl;
       }

};
int main()
{
    Complex c1,c2;

    c1.set_data(3,5);
    c2.set_data(10,50);

    c1.print_data();
    c2.print_data();

    getch();
    return 0;

}
